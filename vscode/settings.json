{
  "editor.fontFamily": "'JetBrains Mono', 'Victor Mono', mononoki, 'DejaVu Sans Mono', MonoLisa, 'Cascadia Code', 'Fira Code', NovaMono, monofur, Hasklig, 'Source Code Pro', Monaco, 'Courier New', monospace",
  "editor.formatOnType": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "editor.tabSize": 2,
  "prettier.requireConfig": true,
  "git.enableSmartCommit": true,
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.jest": true,
    "**/*.flat": true,
    "**/dists": true,
    "**/node_moduless": true,
    "**/obj": true,
    "~": true
  },
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    }
  ],
  "editor.fontLigatures": true,
  "breadcrumbs.enabled": true,
  "search.exclude": {
    "**/.jest": true,
    "**/*.flat": true,
    "**/bin": true,
    "**/bower_components": false,
    "**/dist": true,
    "**/obj": true
  },
  "explorer.confirmDragAndDrop": false,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "terminal.integrated.cursorStyle": "underline",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescript-react]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.console.fontSize": 13,
  "terminal.integrated.fontSize": 16,
  "files.watcherExclude": {
    "**/.jest": true,
    "**/*.flat": true
  },
  "[csharp]": {
    "editor.tabSize": 4
  },
  "editor.detectIndentation": false,
  "vsicons.dontShowNewVersionMessage": true,
  "zenMode.hideLineNumbers": false,
  "extensions.ignoreRecommendations": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "workbench.productIconTheme": "fluent-icons",
  "explorer.compactFolders": false,
  "workbench.colorTheme": "Catppuccin Frappé",
  "explorer.confirmDelete": false,
  "editor.quickSuggestions": {
    "comments": false,
    "strings": false,
    "other": true
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "editor.tabCompletion": "on",
  "editor.formatOnPaste": true,
  "sort-imports.on-save": false,
  "editor.formatOnSave": true,
  "editor.accessibilitySupport": "off",
  "editor.lineHeight": 0,
  "editor.fontSize": 15,
  "emmet.showSuggestionsAsSnippets": true,
  "editor.bracketPairColorization.enabled": true,
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdown.validate.enabled": true,
  "markdown.preview.fontFamily": "'Segoe UI', -apple-system, BlinkMacSystemFont, 'Segoe WPC', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
  "files.associations": {
    "*.mdx": "mdx"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "pnpm",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.semi": false,
  "settingsSync.ignoredExtensions": [
    "lokalise.i18n-ally",
    "kingwl.vscode-vitest-runner"
  ],
  "csharp.unitTestDebuggingOptions": {},
  "markdown.experimental.editor.pasteLinks.enabled": true,
  "editor.experimental.pasteActions.enabled": true,
  "markdown.updateLinksOnFileMove.enabled": "always",
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "editor.unicodeHighlight.allowedCharacters": {
    "×": true,
    "ℹ": true,
    "️": true
  },
  "git.openRepositoryInParentFolders": "never",
  "window.zoomLevel": 1,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true,
  },
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "volar.inlayHints.eventArgumentInInlineHandlers": false,
  "workbench.iconTheme": "ayu",
  "window.menuBarVisibility": "toggle",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.statusBar.visible": false,
}