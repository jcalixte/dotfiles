{
  "editor.fontFamily": "'JetBrains Mono', NovaMono, MonoLisa, mononoki, 'DejaVu Sans Mono', 'Cascadia Code', monofur, 'Fira Code', 'Anonymous Pro', 'Victor Mono', Hasklig, 'Source Code Pro', Monaco, 'Courier New', monospace",
  "editor.formatOnType": true,
  "html.format.endWithNewline": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.tabSize": 2,
  "prettier.requireConfig": true,
  "git.enableSmartCommit": true,
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.jest": true,
    "**/*.flat": true,
    "**/dists": true,
    "**/node_moduless": true,
    "**/obj": true,
    "~": true
  },
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "editor.fontLigatures": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "breadcrumbs.enabled": true,
  "search.exclude": {
    "**/.jest": true,
    "**/*.flat": true,
    "**/bin": true,
    "**/bower_components": false,
    "**/dist": true,
    "**/obj": true
  },
  "explorer.confirmDragAndDrop": false,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": true,
  "terminal.integrated.cursorStyle": "underline",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescript-react]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.console.fontSize": 13,
  "terminal.integrated.fontSize": 16,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.watcherExclude": {
    "**/.jest": true,
    "**/*.flat": true
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.activityBar.visible": true,
  "[csharp]": {
    "editor.tabSize": 4
  },
  "editor.detectIndentation": false,
  "vsicons.dontShowNewVersionMessage": true,
  "zenMode.hideLineNumbers": false,
  "extensions.ignoreRecommendations": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "workbench.productIconTheme": "fluent-icons",
  "explorer.compactFolders": false,
  "workbench.colorTheme": "Palenight (Mild Contrast)",
  "explorer.confirmDelete": false,
  "editor.quickSuggestions": {
    "comments": false,
    "strings": false,
    "other": true
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "editor.tabCompletion": "on",
  "editor.formatOnPaste": true,
  "sort-imports.on-save": false,
  "editor.formatOnSave": true,
  "editor.accessibilitySupport": "off",
  "editor.lineHeight": 0,
  "editor.fontSize": 16,
  "emmet.showSuggestionsAsSnippets": true,
  "editor.bracketPairColorization.enabled": true,
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdown.preview.fontFamily": "'Segoe UI', -apple-system, BlinkMacSystemFont, 'Segoe WPC', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
  "files.associations": {
    "*.mdx": "markdown"
  },
  "window.zoomLevel": 1,
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "pnpm",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.semi": false,
  "settingsSync.ignoredExtensions": [
    "lokalise.i18n-ally"
  ],
  "codetour.showMarkers": false,
  "csharp.unitTestDebuggingOptions": {},
  "markdown.experimental.editor.pasteLinks.enabled": true,
  "editor.experimental.pasteActions.enabled": true,
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
}